{
  "name": "js-shell",
  "description": "Basic JS shell that can be launched via a bookmarklet",
  "author": "James Skinner <spiralx@gmail.com> (https://github.com/spiralx)",
  "version": "0.2.0",
  "license": "MIT",
  "homepage": "https://github.com/spiralx/js-shell",
  "repository": {
    "type": "git",
    "url": "https://github.com/spiralx/js-shell.git"
  },
  "keywords": [
    "javascript",
    "shell",
    "console",
    "bookmarklet",
    "debugging",
    "inspection",
    "jsshell"
  ],
  "scripts": {
    "preinstall": "npm install -g webpack",
    "postinstall": "bower install",
    "jscs": "jscs -m 100 -v -e -r inline src",
    "watch": "webpack -c -d --progress -w",
    "build": "webpack -c -d --progress",
    "stats": "webpack -c -d --progress --json",
    "dist": "webpack -c -p --progress",
    "start": "node app"
  },
  "devDependencies": {
    "babel": "^5.8.20",
    "babel-core": "^5.8.20",
    "babel-eslint": "^4.0.10",
    "babel-loader": "^5.3.2",
    "babel-runtime": "^5.8.20",
    "browser-sync": "^2.7.1",
    "chai": "^2.3.0",
    "css-loader": "^0.12.0",
    "del": "^1.2.1",
    "eslint": "^1.2.0",
    "eslint-loader": "^1.0.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.1",
    "gulp": "^3.9.0",
    "gulp-util": "^3.0.6",
    "jade": "^1.11.0",
    "jade-loader": "^0.7.1",
    "jquery": "^3.2.1",
    "lodash": "^3.10.1",
    "mousetrap": "^1.5.3",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.12.1",
    "stylus-loader": "^1.2.1",
    "url-loader": "^0.5.5",
    "webpack": "^1.11.0"
  }
}
